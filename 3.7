#include <iostream>
using namespace std;

int main() {
    int n;
    cout << "Введіть розмір масиву: ";
    cin >> n;

    if (n < 3) {
        cout << "Масив занадто малий для виконання завдання." << endl;
        return 1;
    }

    int* arr = new int[n];
    int countNegatives = 0;

    cout << "Введіть елементи масиву:" << endl;
    for (int i = 0; i < n; i++) {
        cin >> arr[i];
        if (arr[i] < 0) {
            countNegatives++;
            if (countNegatives == 2) {
                break;
            }
        }
    }

    if (countNegatives < 2) {
        cout << "Масив містить менше ніж два від'ємних елементи." << endl;
    } else {
        cout << "Елементи масиву (крім другого від'ємного елемента):" << endl;
        for (int i = 0; i < n; i++) {
            if (i != 1) {  
                cout << arr[i] << " ";
            }
        }
        cout << endl;
    }

    delete[] arr; 
    return 0;
}
